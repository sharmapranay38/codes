#include<conio.h>
int gcdd(int a,int b){
    int gcd = 1;
    for (int i = 1; i <= a; ++i)
    {
        if (a%i==0 && b%i==0)
        {
            if (gcd<i)
            {
                gcd=i;
            }
        }
    }
    return gcd;
    
}

main(){
    int t;
    scanf("%d",&t);
    while(t--){
    int n;
    scanf("%d",&n);

    for (int a = 1; a < n; ++a)
    {
        int flag=0;
        for (int b = 1; b <n ; ++b)
        {

            int gc = gcdd(a,b);

            if (gc+((a*b)/gc)==n)
            {
                printf("%d %d \n",a,b);
                flag=1;
                break;
            }

        }
        if (flag==1){
            break;
        }


    }
}
}





    


#include <iostream>
using namespace std;

int main() {
    int d;
    int ans;
    scanf("%d",&d);
    if (d>5)
    {
        
        ans = d/5;
        if (d%5==0)
        {
            printf("%d",ans);
        }
        else {
            ans+= 1;
            printf("%d",ans);
        }
        

    }
    else{
        printf("%d",1);
    }
    

}

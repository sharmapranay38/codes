#include<stdio.h>
#include<conio.h>
#include<math.h>
#include <ctype.h>
#include<string.h>
void capital(char s[100]){
    for (int i = 0; s[i]!='\0'; ++i)
    {
        if (s[i] >= 97) {
            s[i] -= 32;
        }
    }
}
main(){
    char a[100],b[100];
    gets(a);
    gets(b);
    capital(a);
    capital(b);
    int i = strcmp(a,b);
    printf("%d",i);

    


}




    


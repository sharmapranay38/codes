#include<stdio.h>
#include<conio.h>
#include<math.h>
int main(){
    int mat[5][5],count=0;
    for (int i = 0; i < 5; ++i)
    {
        for (int j = 0; j < 5; ++j)
        {
            scanf("%d",&mat[i][j]);
            if (mat[i][j]==1){
                count+=abs(2-i);
                count+=abs(2-j);

            }
        }
    }
    printf("%d",count);
}




    

